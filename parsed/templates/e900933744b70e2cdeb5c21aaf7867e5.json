{"components/common/rating-stars":"<div class=\"rating-stars {{#if star_size}}{{star_size}}{{else}}small{{/if}}-stars\" aria-hidden=\"true\">\n  {{#for 1 5}}\n    <svg class=\"rating-star {{#if ../rating '>=' $index}}rating-star-full{{else}}rating-star-empty{{/if}}\" role=\"presentation\">\n      <use xlink:href=\"#icon-star\"></use>\n    </svg>\n  {{/for}}\n</div>\n","components/faceted-search/facets-additional-all":"<ul class=\"collection-facet-list-additional\" data-additional-facets-list>\n  {{#each facets}}\n    {{#if facet '===' ../all_facets}}\n      {{#each items}}\n        <li class=\"collection-facet-list-item\" data-facet-item>\n          <a\n            href=\"{{url}}\"\n            rel=\"nofollow\"\n            data-id=\"{{ id }}\"\n            data-faceted-search-facet>\n            <span class=\"icon {{#if selected}}icon-checked{{else}}icon-unchecked{{/if}}\"></span>\n            {{title}}\n            {{#if ../show_product_counts}}\n              <span class=\"faceted-search-item-count\">\n                ({{count}})\n              </span>\n            {{/if}}\n          </a>\n      </li>\n      {{/each}}\n    {{/if}}\n  {{/each}}\n</ul>","components/faceted-search/facets-additional-toggle":"\n{{#if show_more_toggle}}\n  <div class=\"collection-facet-list-item collection-facet-list-separation\" data-facet-more>\n    <button\n      class=\"collection-facet-list-toggle\"\n      role=\"button\"\n      data-additional-facets=\"{{facet}}\">\n      <span>{{lang 'faceted_search.show_more'}}</span>\n      <span class=\"hidden\">{{lang 'faceted_search.show_less'}}</span>\n    </button>\n  </div>\n{{/if}}","components/faceted-search/facets-base":"\n{{#if visible}}\n  <div class=\"collection-facet filter-type-{{type}}\">\n    <h1 class=\"collection-facet-heading\">{{title}}</h1>\n\n    {{{dynamicComponent 'components/faceted-search/facets'}}}\n  </div>\n{{/if}}\n","components/faceted-search/facets-toggle":"\n<button class=\"collection-toggle-facets\" type=\"button\" data-faceted-search-toggle>\n  {{lang 'collections.filters'}}\n  {{> svg/icon-increment role='button'}}\n</button>\n","components/faceted-search/facets/hierarchy":"<div class=\"collection-facet-list-wrapper\" data-facet-filter-wrapper>\n  <ul\n    data-facet-name=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n    data-has-more-results=\"{{has_more_results}}\">\n\n    {{#if items}}\n      {{#each items}}\n        <li class=\"collection-facet-list-item {{#if selected}}is-active{{/if}}\" data-facet-item>\n          <a href=\"{{#if remove_url}}{{remove_url}}{{else}}{{url}}{{/if}}\" data-faceted-search-facet rel=\"nofollow\">\n            {{title}}\n\n            {{#if ../../show_product_counts}}\n              <span data-facet-item-count>\n                ({{count}})\n              </span>\n            {{/if}}\n          </a>\n        </li>\n      {{/each}}\n    {{else}}\n      <li>{{lang 'faceted_search.empty_facet'}}</li>\n    {{/if}}\n\n  </ul>\n\n   {{> components/faceted-search/facets-additional-toggle}}\n</div>\n","components/faceted-search/facets/multi":"<div class=\"collection-facet-list-wrapper\" data-facet-filter-wrapper>\n  <ul\n    data-facet-name=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n    data-has-more-results=\"{{has_more_results}}\">\n\n    {{#if items}}\n      {{#each items}}\n        <li class=\"collection-facet-list-item {{#if @index '>=' 5}}hidden{{/if}} {{#if selected}}is-active{{/if}}\" data-facet-item>\n          <a href=\"{{#if remove_url}}{{remove_url}}{{else}}{{url}}{{/if}}\" data-faceted-search-facet rel=\"nofollow\">\n            {{title}}\n\n            {{#if ../../show_product_counts}}\n              <span data-facet-item-count>\n                ({{count}})\n              </span>\n            {{/if}}\n          </a>\n        </li>\n      {{/each}}\n    {{else}}\n      <li>{{lang 'faceted_search.empty_facet'}}</li>\n    {{/if}}\n\n  </ul>\n\n   {{> components/faceted-search/facets-additional-toggle}}\n</div>\n","components/faceted-search/facets/range":"<form class=\"collection-facet-range\" method=\"get\" data-facet-filter-wrapper data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{param_name}}\" value=\"{{param_value}}\"/>\n  {{/each}}\n\n  <div class=\"form-field minimum\">\n    <input\n      class=\"form-input\"\n      name=\"min_price\"\n      min=\"0\"\n      type=\"number\"\n      value=\"{{min_price}}\"\n      placeholder=\"{{lang 'faceted_search.min_price'}}\"\n      required\n    />\n  </div>\n\n  <div class=\"form-field maximum\">\n    <input\n      class=\"form-input\"\n      name=\"max_price\"\n      min=\"0\"\n      type=\"number\"\n      value=\"{{max_price}}\"\n      placeholder=\"{{lang 'faceted_search.max_price'}}\"\n      required\n    />\n  </div>\n\n  <div class=\"form-actions\">\n    <input class=\"button button-wide\" type=\"submit\" value=\"{{lang 'faceted_search.update'}}\">\n\n    {{#if remove_url}}\n      <a class=\"button secondary button-wide button-small\" href=\"{{remove_url}}\" rel=\"nofollow\">{{lang 'faceted_search.clear'}}</a>\n    {{/if}}\n  </div>\n\n</form>\n","components/faceted-search/facets/rating":"<ul class=\"collection-facet-list\" data-facet-filter-wrapper>\n  {{#each items}}\n    {{#if param_value}}\n      <li class=\"collection-facet-list-item {{#if selected}}is-active{{/if}}\" data-facet-item>\n        <a href=\"{{#if remove_url}}{{remove_url}}{{else}}{{url}}{{/if}}\" data-faceted-search-facet rel=\"nofollow\">\n          {{> components/common/rating-stars rating=param_value star_size='medium'}}\n\n          {{#if ../../show_product_counts}}{{#if count}}<span data-facet-item-count>({{count}})</span>{{/if}}{{/if}}\n        </a>\n      </li>\n    {{/if}}\n  {{/each}}\n</ul>\n","components/search/sidebar":"\n{{#each product_results.facets}}\n  {{#if partial '!==' 'range'}}\n    {{#if items}}\n      {{> components/faceted-search/facets-base}}\n    {{/if}}\n  {{else}}\n    {{> components/faceted-search/facets-base}}\n  {{/if}}\n{{/each}}\n","svg/icon-increment":"\n{{#unless hide-wrapper}}\n  <svg\n    {{#if role}}role=\"{{role}}\"{{/if}}\n    version=\"1.1\"\n    xmlns=\"http://www.w3.org/2000/svg\"\n    xmlns:xlink=\"http://www.w3.org/1999/xlink\"\n    viewBox=\"0 0 32 32\">\n{{/unless}}\n<path fill-rule=\"evenodd\" class=\"icon-increment-vertical\" d=\"M19.037 3.544h-6.076v24.911h6.076v-24.911z\"></path>\n<path fill-rule=\"evenodd\" class=\"icon-increment-horizontal\" d=\"M3.999 13.266v5.468h24v-5.468h-24z\"></path>\n{{#unless hide-wrapper}}\n  </svg>\n{{/unless}}\n"}
